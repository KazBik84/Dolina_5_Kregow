<% provide(:title, "Klany") %>
<% provide(:header_active, "Legendopedia") %>
<% breadcrumb :legendopedia_clans %>

<%= simple_form_for :clan, { url: show_clan_path }  do |f| %>
  <%= f.input :clan_name, collection: CLAN_NAMES_ARRAY, label: "Klany:", include_blank: false %>
  <%= f.submit "Szukaj!", class: "btn btn-primary" %>
<% end %>
<%= form_tag show_clan_path do %>
  <%= select_tag 'clan_name[]', options_for_select([
    ["Rody Cesarskie", "Rody_Cesarskie"],
    ["Feniks", "Feniks"],
    ["Jednorożec", "Jednorożec"],
    ["Krab", "Krab"],
    ["Lew", "Lew"],
    ["Modliszka", "Modliszka"],
    ["Pająk", "Pająk"],
    ["Skorpion", "Skorpion"],
    ["Smok", "Smok"],
    ["Żuraw", "Żuraw"],
    ["Pomniejsze", "Pomniejsze"]
    ]) %>

  <%= submit_tag "Szukaj!" %>
<% end %>